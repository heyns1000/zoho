GOOGLE_CLIENT_SECRET="your_google_client_secret"
GOOGLE_REFRESH_TOKEN="your_google_refresh_token"

# === GITHUB ===
GITHUB_PERSONAL_ACCESS_TOKEN="ghp_your_github_token"

# === OPENAI ===
OPENAI_API_KEY="sk-your_openai_api_key"

# === ANTHROPIC (Claude) ===
